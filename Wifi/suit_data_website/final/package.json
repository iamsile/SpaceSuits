{
  "name": "gatsby-universal",
  "license": "MIT",
  "author": "Fabian Schultz <desk@fabianschultz.com>",
  "scripts": {
    "develop": "gatsby develop",
    "serve": "gatsby serve",
    "ssr": "npm run build && npm run serve",
    "format": "prettier --config ./.prettierrc --write src/**/*.js",
    "build:dir": "mkdirp ./public/icons/",
    "build:favicons": "node scripts/favicons",
    "build": "npm run build:dir && npm run build:favicons && gatsby build && mv public/index.html public/index.htm",
    "test": "ava **/*.test.js --verbose",
    "start": "node server.js",
    "build-native": "npm run build && concurrently --kill-others \"npm run start\" \"nativefier \"http://localhost:8080\"\" && nativefier \"http://localhost:8080\"",
    "build-cordova": "rm -rf mobile-app/www && rm -rf public && gatsby build && cp -a public/. mobile-app/www/"
  },
  "dependencies": {
    "@reach/dialog": "^0.1.2",
    "@reach/router": "^1.2.1",
    "@reach/visually-hidden": "^0.1.1",
    "axios": "^0.18.0",
    "gatsby": "^2.0.64",
    "gatsby-image": "^2.0.22",
    "gatsby-plugin-offline": "^2.0.19",
    "gatsby-plugin-purgecss": "^3.0.0",
    "gatsby-plugin-react-helmet": "^3.0.4",
    "gatsby-plugin-react-svg": "^2.0.0",
    "gatsby-plugin-sass": "^2.0.7",
    "gatsby-plugin-sharp": "^2.0.15",
    "gatsby-plugin-sitemap": "^2.0.3",
    "gatsby-plugin-styled-components": "^3.0.4",
    "gatsby-plugin-webpack-size": "^0.0.3",
    "gatsby-source-filesystem": "^2.0.11",
    "gatsby-transformer-json": "^2.1.6",
    "gatsby-transformer-remark": "^2.1.15",
    "gatsby-transformer-sharp": "^2.1.9",
    "hash-source": "^1.0.1",
    "history": "^4.7.2",
    "intersection-observer": "^0.5.1",
    "nativefier": "^7.6.9",
    "prop-types": "^15.6.2",
    "reach-router-hash-history": "0.0.3",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-helmet": "^5.2.0",
    "react-pose": "^4.0.4",
    "react-router-dom": "^4.3.1",
    "styled-components": "^4.1.2"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "babel-eslint": "^10.0.0",
    "chrome-launcher": "^0.10.2",
    "concurrently": "^4.1.0",
    "connect": "^3.6.6",
    "core-js": "^2.6.0",
    "directory-named-webpack-plugin": "4.0.0",
    "eslint": "^5.6.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^2.7.0",
    "eslint-plugin-react": "^7.11.1",
    "favicons": "^5.1.1",
    "gatsby-plugin-eslint": "^2.0.1",
    "lighthouse": "^3.0.3",
    "mkdirp": "^0.5.1",
    "node-gyp": "^3.7.0",
    "prettier": "^1.13.5",
    "serve-static": "^1.13.2",
    "why-did-you-update": "0.1.1"
  }
}
